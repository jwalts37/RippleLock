<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tabunlock="http://schemas.android.com/apk/res/com.android.tabunlock" android:orientation="vertical" android:id="@+id/umeng_analyse_rootId" android:background="#efefef" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:background="@drawable/umeng_analyse_feedback_bar_bg" android:layout_width="match_parent" android:layout_height="50dp">
        <TextView android:textSize="22dp" android:textColor="#565656" android:id="@+id/umeng_analyse_feedback_umeng_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="12dp" android:text="反馈问题" android:layout_alignParentLeft="true" android:layout_centerVertical="true"/>
        <ImageButton android:id="@+id/umeng_analyse_feedback_see_list_btn" android:background="@drawable/umeng_analyse_see_list_selector" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="8dp" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
    </RelativeLayout>
    <EditText android:textColor="#737373" android:gravity="top" android:id="@+id/umeng_analyse_feedback_content" android:paddingLeft="9dp" android:paddingTop="10dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginLeft="9dp" android:layout_marginTop="7dp" android:layout_marginRight="9dp" android:layout_marginBottom="2dp" android:hint="请输入您的反馈意见" android:maxEms="10" android:minEms="10" android:layout_weight="1"/>
    <RelativeLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="55dp" android:layout_marginTop="2dp">
        <Spinner android:id="@+id/umeng_analyse_feedback_age_spinner" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="8dp" android:layout_alignParentLeft="true" android:layout_centerVertical="true"/>
        <Spinner android:id="@+id/umeng_analyse_feedback_gender_spinner" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="7dp" android:layout_toRightOf="@+id/umeng_analyse_feedback_age_spinner" android:layout_centerVertical="true"/>
        <TextView android:textColor="#ffffff" android:gravity="center" android:id="@+id/umeng_analyse_feedback_submit" android:background="@drawable/umeng_analyse_submit_selector" android:layout_width="70dp" android:layout_height="43dp" android:layout_marginRight="8dp" android:text="提交" android:layout_alignBaseline="@+id/umeng_analyse_feedback_age_spinner" android:layout_alignParentRight="true"/>
    </RelativeLayout>
</LinearLayout>
